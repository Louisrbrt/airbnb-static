<html>
  <head>
    <meta charset="utf-8">
    <title>Airbnb home</title>
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,700|Raleway:300,400,500,700">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="css/style.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- Leave those next 4 lines if you care about users using IE8 -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>

    <!-- ~~~~~~~~~~~~~~~~~~~~~ -->
    <!-- BANNER -->
    <!-- ~~~~~~~~~~~~~~~~~~~~~ -->
    <div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('images/dog-phone.jpg');">
      <div class="banner-content">
        <h1>DogBnB</h1>
        <p>The most confy dog houses</p>
        <%= link_to "WOOF IT", "/", class: "btn btn-primary btn-lg" %>
      </div>
    </div>

    <!-- ~~~~~~~~~~~~~~~~~~~~~ -->
    <!-- FEATURES -->
    <!-- ~~~~~~~~~~~~~~~~~~~~~ -->
    <div class="features">
      <div class="container features-container">
        <div class="row">
          <div class="col-md-3">
            <div class="feature-card text-center">
              <%= image_tag "dollar.png" %>
              <h3>Some woofy feature</h3>
              <p>Crazy sheet here dude. totally worth a woof !</p>
          </div>
          </div>
          <div class="col-md-3">
            <div class="feature-card text-center">
              <%= image_tag "dollar.png" %>
              <h3>Some woofy feature</h3>
              <p>Crazy sheet here dude. totally worth a woof !</p>
          </div>
          </div>
          <div class="col-md-3">
            <div class="feature-card text-center">
              <%= image_tag "dollar.png" %>
              <h3>Some woofy feature</h3>
              <p>Crazy sheet here dude. totally worth a woof !</p>
          </div>
          </div>
          <div class="col-md-3">
            <div class="feature-card text-center">
              <%= image_tag "dollar.png" %>
              <h3>Some woofy feature</h3>
              <p>Crazy sheet here dude. totally worth a woof !</p>
          </div>
          </div>
        </div>
      </div>
    </div>


    <!-- ~~~~~~~~~~~~~~~~~~~~~ -->
    <!-- CARDS -->
    <!-- ~~~~~~~~~~~~~~~~~~~~~ -->
    <div class="cards-wrapper">
      <div class="container card-container">
        <div class="row">
          <div class="col-xs-12 col-sm-4">
            <!-- insert <div class="card"> -->
            <div class="card" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-brussels.jpeg');">
              <div class="card-category">Popular</div>
              <div class="card-description">
                <h2>Some stuff</h2>
                <p>Very cool stuff, useful and smart</p>
              </div>
              <%= image_tag "romain.jpeg", class:"card-user avatar" %>
              <%= link_to "#", "/", class:"card-link" %>
            </div>
          </div>
          <div class="col-xs-12 col-sm-4">
            <!-- insert <div class="card"> -->
            <div class="card" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-paris.jpg');">
              <div class="card-category">Popular</div>
              <div class="card-description">
                <h2>Some stuff</h2>
                <p>Very cool stuff, useful and smart</p>
              </div>
              <%= image_tag "seb.jpg", class:"card-user avatar" %>
              <%= link_to "#", "/", class:"card-link" %>
            </div>
          </div>
          <div class="col-xs-12 col-sm-4">
            <!-- insert <div class="card"> -->
            <div class="card" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-london.jpeg');">
              <div class="card-category">Popular</div>
              <div class="card-description">
                <h2>Some stuff</h2>
                <p>Very cool stuff, useful and smart</p>
              </div>
              <%= image_tag "anne.jpg", class:"card-user avatar" %>
              <%= link_to "#", "/", class:"card-link" %>
            </div>
          </div>
        </div>
      </div>
    </div>


    <!-- ~~~~~~~~~~~~~~~~~~~~~ -->
    <!-- NEWSLETTER FORM -->
    <!-- ~~~~~~~~~~~~~~~~~~~~~ -->
    <div class="form-wrapper">
      <div class="container form-container text-center">
        <h2>Wanna stay in touch buddy ?</h2>
        <p>Leave your doggy mail</p>
        <form action="#" class="form-inline">
          <div class="form-group">
            <input type="text" id="email" placeholder="woof@doggy.com" class="form-control">
            <input type="submit" class="btn btn-primary">
          </div>
        </form>
      </div>
    </div>


    <!-- ~~~~~~~~~~~~~~~~~~~~~ -->
    <!-- MAP -->
    <!-- ~~~~~~~~~~~~~~~~~~~~~ -->
    <div id="map" style="width: 100%; height: 400px;"></div>

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

    <script type="text/javascript">
    var myLatlng = new google.maps.LatLng(48.8655909, 2.378983);

    var myOptions = {
      zoom: 16,
      center: myLatlng,
      scrollwheel: false,
      mapTypeId: google.maps.MapTypeId.ROADMAP,
      styles: [] // TODO: replace [] by array from https://snazzymaps.com/
    };

    var map = new google.maps.Map(document.getElementById('map'), myOptions);
    var marker = new google.maps.Marker({
      position: myLatlng,
      map: map,
      title: "You are here!"
    });
    </script>


    <!-- Including Bootstrap JS (with its jQuery dependency) so that dynamic components work -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
  </body>
</html>
